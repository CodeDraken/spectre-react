{
  "name": "spectre-react",
  "version": "0.3.0",
  "description": "React components for Spectre.css",
  "main": "dist",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeDraken/spectre-react.git"
  },
  "homepage": "https://codedraken.github.io/spectre-react/",
  "standard": {
    "ignore": [
      "node_modules/**"
    ],
    "parser": "babel-eslint",
    "envs": [
      "jest",
      "node"
    ]
  },
  "scripts": {
    "build": "parcel build --experimental-scope-hoisting src/index.js",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "storybook-deploy": "storybook-to-ghpages",
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "standard 'src/**/*.js!(*.snap)'",
    "lint-fix": "standard 'src/**/*.js!(*.snap)' --fix"
  },
  "keywords": [
    "react",
    "ui",
    "spectre",
    "components"
  ],
  "author": "CodeDraken",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^3.4.10",
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-backgrounds": "^3.4.10",
    "@storybook/addon-centered": "^3.4.10",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-events": "^3.4.10",
    "@storybook/addon-info": "^3.4.10",
    "@storybook/addon-knobs": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addon-notes": "^3.4.10",
    "@storybook/addon-options": "^3.4.10",
    "@storybook/addon-storyshots": "^3.4.10",
    "@storybook/addon-storysource": "^3.4.10",
    "@storybook/addon-viewport": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/cli": "^3.4.10",
    "@storybook/components": "^3.4.10",
    "@storybook/core-events": "^4.0.0-alpha.11",
    "@storybook/node-logger": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.6.0",
    "jsdom": "^12.2.0",
    "parcel-bundler": "^1.10.3",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2"
  }
}
